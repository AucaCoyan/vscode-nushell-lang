{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "nushell",
  "scopeName": "source.nushell",
  "patterns":
  [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#punctuation"
    },
    {
      "include": "#comment"
    },
    {
      "include": "#range"
    },
    {
      "include": "#parameter"
    },
    {
      "include": "#digit"
    },
    {
      "include": "#blockparam"
    },
    {
      "include": "#variable"
    },
    {
      "include": "#function"
    },
    {
      "include": "#type"
    }
  ],
  "repository":
  {
    "keywords":
    {
      "patterns":
      [
        {
          "name": "keyword.other.a",
          "match": "\\bansi(\\s|\\sgradient|\\sstrip)?\\b"
        },
        {
          "name": "keyword.other.a",
          "match": "\\bappend(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.a",
          "match": "\\b(agg|agg-groups|alias|all-false|all-true|all\\?|any\\?|arg-max|arg-min|arg-sort|arg-true|arg-unique|as|as-date|as-datetime)\\b"
        },
        {
          "name": "keyword.other.b",
          "match": "\\b(benchmark|build-string)\\b"
        },
        {
          "name": "keyword.other.c",
          "match": "\\bcollect(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.c",
          "match": "\\bconfig(\\s|\\senv|\\snu)?\\b"
        },
        {
          "name": "keyword.other.c",
          "match": "\\b(cache|cal|cd|char|clear|col|columns|compact|complete|concatenate|contains|count|count-null|cp|cumulative)\\b"
        },
        {
          "name": "keyword.other.d",
          "match": "\\bdate(\\s|\\sformat|\\shumanize|\\slist-timezone|\\snow|\\sto-record|\\sto-table|\\sto-timezone)?\\b"
        },
        {
          "name": "keyword.other.d",
          "match": "\\bdb(\\s|\\sand|\\sas|\\scol|\\scollect|\\sdescribe|\\sfn|\\sfrom|\\sgroup-by|\\sjoin|\\slimit|\\sopen|\\sor|\\sorder-by|\\sover|\\squery|\\sschema|\\sselect|\\stesting|\\swhere)?\\b"
        },
        {
          "name": "keyword.other.d",
          "match": "\\bdescribe(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.d",
          "match": "\\bdrop(\\s|\\s|\\scolumn|\\snth)?\\b"
        },
        {
          "name": "keyword.other.d",
          "match": "\\b(debug|decode|def|def-env|default|detect|df-not|do|drop-duplicates|drop-nulls|dtypes|du)\\b"
        },
        {
          "name": "keyword.other.e",
          "match": "\\beach(\\s|\\swhile)?\\b"
        },
        {
          "name": "keyword.other.e",
          "match": "\\bexport(\\s|\\salias|\\sdef|\\sdef-env|\\senv|\\sextern)?\\b"
        },
        {
          "name": "keyword.other.e",
          "match": "\\b(echo|empty\\?|enter|env|error|every|exec|exit|explode|expr-not|extern)\\b"
        },
        {
          "name": "keyword.other.f",
          "match": "\\bfetch(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.f",
          "match": "\\bfirst(\\s|\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.f",
          "match": "\\bflatten(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.f",
          "match": "\\bformat(\\s|\\sfilesize)?\\b"
        },
        {
          "name": "keyword.other.f",
          "match": "\\bfrom(\\s|\\scsv|\\seml|\\sics|\\sini|\\sjson|\\snuon|\\sods|\\sssv|\\stoml|\\stsv|\\surl|\\svcf|\\sxlsx|\\sxml|\\syaml|\\syml)?\\b"
        },
        {
          "name": "keyword.other.f",
          "match": "\\b(fill-na|fill-null|filter-with|find|fmt|for)\\b"
        },
        {
          "name": "keyword.other.g",
          "match": "\\bget(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.g",
          "match": "\\bgroup-by(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.g",
          "match": "\\b(g|get-day|get-hour|get-minute|get-month|get-nanosecond|get-ordinal|get-second|get-week|get-weekday|get-year|glob|grid|group|gstat)\\b"
        },
        {
          "name": "keyword.other.h",
          "match": "\\bhash(\\s|\\sbase64|\\smd5|\\ssha256)?\\b"
        },
        {
          "name": "keyword.other.h",
          "match": "\\b(headers|help|hide|histogram|history)\\b"
        },
        {
          "name": "keyword.other.i",
          "match": "\\binto(\\s|\\sbinary|\\sbool|\\sdatetime|\\sdecimal|\\sduration|\\sfilesize|\\sint|\\sstring)?\\b"
        },
        {
          "name": "keyword.other.i",
          "match": "\\bis-not-null(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.i",
          "match": "\\bis-null(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.i",
          "match": "\\b(if|ignore|inc|input|insert|is-admin|is-duplicated|is-in|is-unique)\\b"
        },
        {
          "name": "keyword.other.j",
          "match": "\\b(join)\\b"
        },
        {
          "name": "keyword.other.k",
          "match": "\\bkeep(\\s|\\suntil|\\swhile)?\\b"
        },
        {
          "name": "keyword.other.k",
          "match": "\\bkeybindings(\\s|\\sdefault|\\slist|\\slisten)?\\b"
        },
        {
          "name": "keyword.other.k",
          "match": "\\b(kill)\\b"
        },
        {
          "name": "keyword.other.l",
          "match": "\\blast(\\s|\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.l",
          "match": "\\b(length|let|let-env|lines|list|lit|load-env|ls|ls-df)\\b"
        },
        {
          "name": "keyword.other.m",
          "match": "\\bmath(\\s|\\sabs|\\savg|\\sceil|\\seval|\\sfloor|\\smax|\\smedian|\\smin|\\smode|\\sproduct|\\sround|\\ssqrt|\\sstddev|\\ssum|\\svariance)?\\b"
        },
        {
          "name": "keyword.other.m",
          "match": "\\bmax(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.m",
          "match": "\\bmean(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.m",
          "match": "\\bmedian(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.m",
          "match": "\\bmin(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.m",
          "match": "\\b(match|melt|merge|metadata|mkdir|module|move|mv)\\b"
        },
        {
          "name": "keyword.other.n",
          "match": "\\bn-unique(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.n",
          "match": "\\b(n|nth|nu-highlight)\\b"
        },
        {
          "name": "keyword.other.o",
          "match": "\\boverlay(\\s|\\sadd|\\slist|\\snew|\\sremove)?\\b"
        },
        {
          "name": "keyword.other.o",
          "match": "\\b(open|open-df|otherwise)\\b"
        },
        {
          "name": "keyword.other.p",
          "match": "\\bpath(\\s|\\sbasename|\\sdirname|\\sexists|\\sexpand|\\sjoin|\\sparse|\\srelative-to|\\ssplit|\\stype)?\\b"
        },
        {
          "name": "keyword.other.p",
          "match": "\\b(p|par-each|parse|pivot|post|prepend|print|ps)\\b"
        },
        {
          "name": "keyword.other.q",
          "match": "\\bquantile(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.q",
          "match": "\\bquery(\\s|\\sjson|\\sweb|\\sxml)?\\b"
        },
        {
          "name": "keyword.other.r",
          "match": "\\brandom(\\s|\\sbool|\\schars|\\sdecimal|\\sdice|\\sinteger|\\suuid)?\\b"
        },
        {
          "name": "keyword.other.r",
          "match": "\\brename(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.r",
          "match": "\\breverse(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.r",
          "match": "\\broll(\\s|\\sdown|\\sleft|\\sright|\\sup)?\\b"
        },
        {
          "name": "keyword.other.r",
          "match": "\\b(range|reduce|register|reject|replace|replace-all|rm|rolling|rotate|run-external)\\b"
        },
        {
          "name": "keyword.other.s",
          "match": "\\bselect(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.s",
          "match": "\\bseq(\\s|\\schar|\\sdate)?\\b"
        },
        {
          "name": "keyword.other.s",
          "match": "\\bskip(\\s|\\suntil|\\swhile)?\\b"
        },
        {
          "name": "keyword.other.s",
          "match": "\\bsort-by(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.s",
          "match": "\\bsplit(\\s|\\schars|\\scolumn|\\srow)?\\b"
        },
        {
          "name": "keyword.other.s",
          "match": "\\bstd(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.s",
          "match": "\\bstr(\\s|\\scamel-case|\\scapitalize|\\scollect|\\scontains|\\sdowncase|\\sends-with|\\sfind-replace|\\sindex-of|\\skebab-case|\\slength|\\slpad|\\spascal-case|\\sreplace|\\sreverse|\\srpad|\\sscreaming-snake-case|\\ssnake-case|\\sstarts-with|\\ssubstring|\\stitle-case|\\sto-datetime|\\sto-decimal|\\sto-int|\\strim|\\supcase)?\\b"
        },
        {
          "name": "keyword.other.s",
          "match": "\\bsum(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.s",
          "match": "\\b(sample|save|set|set-with-idx|shape|shells|shift|shuffle|size|sleep|slice|sort|source|split-by|str-lengths|str-slice|strftime|sys)\\b"
        },
        {
          "name": "keyword.other.t",
          "match": "\\btake(\\s|\\s|\\suntil|\\swhile)?\\b"
        },
        {
          "name": "keyword.other.t",
          "match": "\\bto(\\s|\\scsv|\\shtml|\\sjson|\\smd|\\snuon|\\stext|\\stoml|\\stsv|\\surl|\\sxml|\\syaml)?\\b"
        },
        {
          "name": "keyword.other.t",
          "match": "\\bto-nu(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.t",
          "match": "\\b(table|term|to-csv|to-df|to-dummies|to-lazy|to-lowercase|to-parquet|to-uppercase|touch|transpose|tutor)\\b"
        },
        {
          "name": "keyword.other.u",
          "match": "\\bupdate(\\s|\\scells)?\\b"
        },
        {
          "name": "keyword.other.u",
          "match": "\\burl(\\s|\\shost|\\spath|\\squery|\\sscheme)?\\b"
        },
        {
          "name": "keyword.other.u",
          "match": "\\b(unalias|uniq|unique|upsert|use)\\b"
        },
        {
          "name": "keyword.other.v",
          "match": "\\bvar(\\s|\\s)?\\b"
        },
        {
          "name": "keyword.other.v",
          "match": "\\b(value-counts|version|view-source)\\b"
        },
        {
          "name": "keyword.other.w",
          "match": "\\b(watch|when|where|which|window|with-column|with-env|wrap)\\b"
        },
        {
          "name": "keyword.other.z",
          "match": "\\b(zip)\\b"
        }
      ]
    },
    "strings":
    {
      "name": "string.quoted.double.nushell",
      "begin": "\"|'|`",
      "end": "\"|'|`"
    },
    "variable":
    {
      "match": "(alias|let|let-env)\\s+[\\$'\"]?([a-z A-Z0-9_-]+)['\"]?|(\\$[a-zA-Z0-9_\\-]+)|(\\|)([$a-zA-Z0-9-]+)[\\s,]+([$a-zA-Z0-9-]+)(\\|)",
      "name": "variable.nushell",
      "captures":
      {
        "1":
        {
          "name": "keyword.other.nushell"
        },
        "2":
        {
          "name": "variable.name.nushell"
        },
        "3":
        {
          "name": "variable.name.nushell"
        },
        "4":
        {
          "name": "punctuation.nushell"
        },
        "5":
        {
          "name": "variable.name.nushell"
        },
        "6":
        {
          "name": "variable.name.nushell"
        },
        "7":
        {
          "name": "punctuation.nushell"
        }
      }
    },
    "punctuation":
    {
      "patterns":
      [
        {
          "comment": "comma",
          "name": "punctuation.comma.nushell",
          "match": ","
        },
        {
          "comment": "curly braces",
          "name": "punctuation.brackets.curly.nushell",
          "match": "[{}]"
        },
        {
          "comment": "parentheses, round brackets",
          "name": "punctuation.brackets.round.nushell",
          "match": "[()]"
        },
        {
          "comment": "semicolon",
          "name": "punctuation.semi.nushell",
          "match": ";"
        },
        {
          "comment": "square brackets",
          "name": "punctuation.brackets.square.nushell",
          "match": "[\\[\\]]"
        },
        {
          "comment": "angle brackets",
          "name": "punctuation.brackets.angle.nushell",
          "match": "(?<!=)[<>]"
        }
      ]
    },
    "comment":
    {
      "match": "(#.*)",
      "name": "comment.nushell"
    },
    "range":
    {
      "match": "(\\d+\\.\\.\\d+)",
      "name": "constant.nushell"
    },
    "digit":
    {
      "match": "(\\b[0-9]+\\b)",
      "name": "constant.numeric.nushell"
    },
    "parameter":
    {
      "match": "(\\s-[a-zA-Z_\\-]|--[a-zA-Z_\\-]{1,50})",
      "name": "variable.parameter.function.nushell"
    },
    "blockparam":
    {
      "match": "(\\|)([a-zA-Z0-9\\-\\_]+)(\\|)",
      "name": "variable.nushell",
      "captures":
      {
        "2":
        {
          "name": "variable.language.nushell"
        }
      }
    },
    "function":
    {
      "match": "\\b(def|def-env|extern)\\s+([\"'a-zA-Z0-9_\\- ]{1,})",
      "name": "entity.name",
      "captures":
      {
        "1":
        {
          "name": "entity.name.function.nushell"
        },
        "2":
        {
          "name": "entity.name.type.nushell"
        }
      }
    },
    "type":
    {
      "match": "([a-zA-Z0-9\\-_ ]+)(\\:[A-Za-z0-9\\-_ ]{1,})",
      "name": "storage",
      "captures":
      {
        "1":
        {
          "name": "storage.modifier.nushell"
        },
        "2":
        {
          "name": "storage.type.nushell"
        }
      }
    }
  }
}