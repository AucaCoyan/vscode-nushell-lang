{
  "name": "vscode-nushell-lang",
  "displayName": "vscode-nushell-lang",
  "description": "nushell language for vscode",
  "version": "0.1.0",
  "license": "MIT",
  "publisher": "nushell-team",
  "repository": {
    "type": "git",
    "url": "https://github.com/nushell/vscode-nushell-lang.git"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "icon": "assets/nushell.ico",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nushell",
        "aliases": [
          "nushell",
          "nu"
        ],
        "extensions": [
          ".nu"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nushell",
        "scopeName": "source.nushell",
        "path": "./syntaxes/nushell.tmLanguage.json"
      }
    ],
    "themes": [
      {
          "label": "Nushell-Dark",
          "id": "nushell",
          "uiTheme": "vs-dark",
          "path": "./nushell.theme-minimal-dark.json"
      },
      {
        "label": "Nushell-Light",
        "id": "nushell",
        "uiTheme": "vs",
        "path": "./nushell.theme-minimal-light.json"
    }
  ]
  }
}
